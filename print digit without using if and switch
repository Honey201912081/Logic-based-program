/*
123
one two three
*/

#include<iostream>
using namespace std;
int main()

{
        string arr[10]={"zero","one","two","three","four","five","six","seven","eight","nine"};
        string ts="";
        int n,r;
        cin>>n;
        while(n!=0)
        {
            r=n%10;
            ts=arr[r]+" "+ts;
            n=n/10;
        }
        
        cout<<ts;
        return 0;
}
